inActions>\r\n            <ButtonLink to={`/users/${user.id}/edit`}>Edit user</ButtonLink>\r\n        <UserDeleteButton userId={user.id} />\r\n        <ResetDataButton/>\r\n        </AdminActions>\r\n      \r\n    <Grid templateColumns=\"repeat(5, 1fr)\" gap={2}>\r\n      <GridItem colSpan={2} h=\"10\">\r\n        \r\n      <Heading as=\"h6\" size=\"lg\" textAlign=\"center\">\r\n         Owner books\r\n      </Heading>\r\n        <Flex wrap=\"wrap\">\r\n         {user.ownedBookCopies.map(bookCopy => <BookCopy key={bookCopy.id} bookCopy={bookCopy} showOwner showBorrower showActions/>)}\r\n        </Flex>\r\n        </GridItem>\r\n\r\n        \r\n        \r\n        <GridItem colStart={4} colEnd={6} h=\"10\">\r\n          <Heading as=\"h5\" size=\"lg\" textAlign=\"center\">\r\n        Borrowed books\r\n       </Heading>\r\n        <Flex wrap=\"wrap\">\r\n         {user.borrowedBookCopies.map(bookCopy => <BookCopy key={bookCopy.id} bookCopy={bookCopy} showOwner showBorrower showActions/>)}\r\n        </Flex>\r\n      </GridItem>\r\n      </Grid>\r\n      </>\r\n    \r\n  );\r\n}\r\n\r\n","c:\\dev_graphQL\\Library_application\\library-list-authors\\src\\pages\\AuthorDetailsPage.js",[],"c:\\dev_graphQL\\Library_application\\library-list-authors\\src\\components\\Link.js",[],"c:\\dev_graphQL\\Library_application\\library-list-authors\\src\\components\\UserDetails.js",[],"c:\\dev_graphQL\\Library_application\\library-list-authors\\src\\components\\BookDetails.js",[],"c:\\dev_graphQL\\Library_application\\library-list-authors\\src\\components\\AuthorDetails.js",[],"c:\\dev_graphQL\\Library_application\\library-list-authors\\src\\components\\SearchBox.js",[],"c:\\dev_graphQL\\Library_application\\library-list-authors\\src\\components\\BookCopy.js",[],"c:\\dev_graphQL\\Library_application\\library-list-authors\\src\\components\\Avatar.js",[],"c:\\dev_graphQL\\Library_application\\library-list-authors\\src\\components\\BookCopy\\BookCopyActions.js",[],"c:\\dev_graphQL\\Library_application\\library-list-authors\\src\\components\\BookCopy\\BorrowButton.js",["348"],"import { gql, useMutation } from \"@apollo/client\";\r\nimport { Button, useToast } from \"@chakra-ui/react\";\r\nimport React from \"react\";\r\nimport { GET_USER_QUERY } from \"../../pages/UserDetailsPage\";\r\nimport { BOOK_COPY_FIELDS_FRAGMENT } from './fragments';\r\n\r\nconst BORROW_BOOK_COPY_MUTATION = gql`\r\nmutation BorrowBookCopy($bookCopyId: ID!) {\r\n  borrowBookCopy(id: $bookCopyId) {\r\n    id\r\n    ...bookCopyFields\r\n\r\n  }\r\n\r\n\r\n}\r\n${BOOK_COPY_FIELDS_FRAGMENT}\r\n`;\r\n\r\nexport default function BorrowButton({ availableBookCopy }) {\r\n\r\n  const toast = useToast();\r\n  const [borrowBookcopy, { loading, data }] = useMutation(BORROW_BOOK_COPY_MUTATION, {\r\n    variables: { bookCopyId: availableBookCopy.id },\r\n    onCompleted: () => {\r\n      toast({\r\n        title: \"Success\",\r\n        description: \"You've borrowed the book\",\r\n        status: \"success\",\r\n        duration: 1000,\r\n        position: \"top\",\r\n        isClosable: true\r\n      });\r\n    },\r\n    onError: error => {\r\n      toast({\r\n        title: \"Could not borrow the book\",\r\n        description: error.message,\r\n        status: \"error\",\r\n        duration: 1000,\r\n        position: \"top\",\r\n        isClosable: true\r\n      });\r\n    },\r\n    refetchQueries: ({ data }) => {\r\n      return [\r\n        {\r\n          query: GET_USER_QUERY,\r\n          variables: { userId: data.borrowBookCopy.borrower.id }\r\n        }\r\n      ];\r\n    }\r\n\r\n  });\r\n\r\n  return (\r\n    <Button disabled={loading} onClick={borrowBookcopy}>\r\n      Borrow\r\n    </Button>\r\n  );\r\n}","c:\\dev_graphQL\\Library_application\\library-list-authors\\src\\components\\BookCopy\\ReturnButton.js",[],"c:\\dev_graphQL\\Library_application\\library-list-authors\\src\\components\\BookCopy\\fragments.js",[],"c:\\dev_graphQL\\Library_application\\library-list-authors\\sr